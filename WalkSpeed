function check()
	for i,v in pairs(workspace:GetDescendants()) do
		if v.Name:lower() == "speed" then
			v.Touched:Connect(function(hit)
				local hum = hit.Parent:WaitForChild("Humanoid")
				if hum ~= nil then
					if script:FindFirstChild("Speed") ~= nil then
						hum.WalkSpeed = script:FindFirstChild("Speed").Value
					end
				end
			end)
		end
	end
end

while wait() do
	check()
end
